in package test

enum Color:/ Black, Red
bitmask Recipe:/ HasSugar, HasSalt, HasPepper, HasVinegar

interface interface_class:/
    void can_do()

class base_of_base:/
    l: float32

class base_class: base_of_base, implements={interface_class}:/
    m: int32
    void can_do()

class sub_class: base_class:/
    n: int32

struct struct_w_ref:/
    m: base_class

struct struct_w_interface_ref:/
    m: interface_class

test/suite boolean:/
    void tc_true()
    void tc_false()
    void tc_int_zero()
    void tc_int_nonzero()
    void tc_string()
    void tc_reference()
    void tc_reference_value_type()
    void tc_reference_value_type_true()
    void tc_null()
    void tc_constant_zero()
    void tc_constant_nonzero()

test/suite character:/
    void tc_alpha()
    void tc_digit()
    void tc_newline()
    void tc_null_character()
    void tc_number()
    void tc_zero()

test/suite unsigned_int :/
    void tc_signed()
    void tc_unsigned()
    void tc_floating_point()
    void tc_negative_floating_point()
    void tc_floating_point_scientific()
    void tc_hexadecimal()
